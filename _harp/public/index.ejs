<h1>Welcome to Harp.</h1>
<h3>This is yours to own. Enjoy.</h3>

<% var blogs = public._data; -%>

<ul>
    <% for (var blog in blogs) { -%>
        <li>
            <a href="<%= blog %>"><%= blogs[blog].title %></a>
            <br>
            <%= blogs[blog].date %>
            <br>
            <%= blogs[blog].description %>
        </li>
    <% } -%>
</ul>